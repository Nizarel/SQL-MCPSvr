{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "MCP_Azsql": "Information"
    }  },
  "AllowedHosts": "*",  "CacheOptions": {
    "DefaultExpiration": "00:05:00",
    "SlidingExpiration": "00:02:00",
