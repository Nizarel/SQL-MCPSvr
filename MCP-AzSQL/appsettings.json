  "ApplicationInsights": {
    "InstrumentationKey": "",
    "ConnectionString": ""
  },
  "HealthChecks": {
    "UI": {
      "HealthCheckDatabaseConnectionString": "",
      "EvaluationTimeOnSeconds": 10,
      "MinimumSecondsBetweenFailureNotifications": 60
    }
  },
  "Azure": {
    "Environment": "Production",
    "Region": "",
    "SubscriptionId": "",
    "ResourceGroup": "",
    "ContainerApp": {
      "Name": "mcp-mssql-server",
      "RevisionSuffix": "",
      "EnvironmentName": "mcp-environment"
    }
  },
  "Database": {
    "CommandTimeout": 30,
    "MaxRetryAttempts": 3,
    "RetryDelaySeconds": 2
  }
